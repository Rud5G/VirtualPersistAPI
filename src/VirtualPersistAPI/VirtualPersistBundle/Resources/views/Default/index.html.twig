{% extends '::base.html.twig' %}
{% block body %}
<div class="container-fluid">
<div class="row-fluid">
<div class="span2">
</div>
<div class="span10">
<h2>{{ header }}</h2>
<p>This VirtualPersistAPI site has the following users:</p>
<ul>
    {% for user in users %}
        <li>{{ user.uuid|e }}</li>
    {% endfor %}
</ul>
<p>Categories:</p>
<ul>
    {% for category in categories %}
        <li>{{ category['category']|e }}</li>
    {% endfor %}
</ul>
</div>
</div>
</div>
{% endblock %}

